  <log4net>
    <appender name="LogFileAppender" type="log4net.Appender.FileAppender">
      <param name="File" value="data\Logs\\Log4Net.log"/>
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%d [%t] %-5p %c %m%n"/>
      </layout>
    </appender>

    <appender name="InformationAppender" type="log4net.Appender.FileAppender">
      <param name="File" value="data\Logs\\Information.log"/>
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%d [%t] %-5p %c %m%n"/>
      </layout>
    </appender>
	
	    <appender name="FabricPackAppender" type="log4net.Appender.FileAppender">
      <param name="File" value="data\Logs\\FabricPack.log"/>
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%d [%t] %-5p %c %m%n"/>
      </layout>
    </appender>

    <appender name="ExceptionAppender" type="log4net.Appender.FileAppender">
      <param name="File" value="data\Logs\\Exception.log"/>
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%d [%t] %-5p %c %m%n"/>
      </layout>
    </appender>

    <appender name="LogOrderAppender" type="log4net.Appender.FileAppender">
      <param name="File" value="data\Logs\\Order.log"/>
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%d [%t] %-5p %c %m%n"/>
      </layout>
    </appender>

    <appender name="EmailLogFileAppender" type="log4net.Appender.FileAppender">
      <param name="File" value="data\Logs\\Email.log"/>
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%d [%t] %-5p %c %m%n"/>
      </layout>
    </appender>

    <appender name="SmtpAppender" type="log4net.Appender.SmtpAppender">
      <to value="" />
      <from value="" />
      <subject value="" />
      <smtpHost value="" />
      <bufferSize value="512" />
      <lossy value="true" />
      <evaluator type="log4net.Core.LevelEvaluator">
        <threshold value="WARN"/>
      </evaluator>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%newline%date [%thread] %-5level %logger [%property] - %message%newline%newline%newline" />
      </layout>
    </appender>

    <logger name="File">
      <level value="All" />
      <appender-ref ref="LogFileAppender" />
    </logger>
    <logger name="Order">
      <level value="All" />
      <appender-ref ref="LogOrderAppender" />
    </logger>
    <logger name="Email">
      <level value="All" />
      <appender-ref ref="EmailLogFileAppender" />
    </logger>
    <logger name="Information">
      <level value="All" />
      <appender-ref ref="InformationAppender" />
    </logger>
	    <logger name="FabricPack">
      <level value="All" />
      <appender-ref ref="FabricPackAppender" />
    </logger>

    <logger name="Exception">
      <level value="All" />
      <appender-ref ref="ExceptionAppender" />
    </logger>

<logger name="SMTP">
      <level value="All" />
      <appender-ref ref="SmtpAppender" />
    </logger>
    
    <logger name="EmailLog">
      <level value="All" />
      <appender-ref ref="SmtpAppender" />
    </logger>
    
  </log4net>

 
  
