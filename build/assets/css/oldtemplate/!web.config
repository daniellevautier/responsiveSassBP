    <sectionGroup name="commerceDomainConfiguration" type="System.Configuration.SingleTagSectionHandler" >
      <section name="localhost" type="System.Configuration.SingleTagSectionHandler" allowDefinition="MachineToApplication" restartOnExternalChanges="true" requirePermission="false" />
